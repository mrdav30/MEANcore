    "passport-local": "~1.0.0",
    "passport-paypal-openidconnect": "^0.1.1",
    "passport-twitter": "~1.0.4",
    "pngquant": "^4",
    "puppeteer": "^10.1",
    "rxjs": "^6.6.0",
    "sitemap-generator": "^8.5.1",
    "socket.io": "^4.1.2",
    "streamifier": "^0.1.1",
    "tslib": "^2.0.1",
