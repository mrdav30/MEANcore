    "zone.js": "^0.11.1",
    "snyk": "^1.385.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.7",
    "@angular/cli": "^10.0.7",
    "@angular/compiler-cli": "^10.0.12",
    "@angular/language-service": "^10.0.12",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^3.5.1",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.149",
