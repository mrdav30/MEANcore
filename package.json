    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.8.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^7",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
