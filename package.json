    "bootstrap": "^4.4.1",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "concat-stream": "^2.0.0",
    "connect-flash": "~0.1.1",
    "connect-mongo": "^3.2.0",
    "connect-redis": "^4.0.3",
    "cookie-parser": "~1.4.1",
    "core-js": "^3.6.4",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
