{
  "name": "meancore",
  "description": "Full-Stack JavaScript  with MongoDB, Express, AngularJS, and Node.js.",
  "version": "0.5.0-beta",
  "meancore-version": "0.5.0-beta",
  "private": false,
  "author": "david.oravsky",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/meancore/meancore.git"
  },
  "engines": {
    "node": ">=5",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "ng": "ng",
    "config": "node ./scripts/set-env.ts",
    "version": "node ./scripts/version.ts",
    "build:dev": "npm run config && npm run version && ng build",
    "build:prod": "npm run config && npm run version && ng build -c production",
    "client:dev": "ng serve --aot --progress=false --proxy-config proxy.conf.js",
    "client:prod": "ng serve -c production --progress=false --proxy-config proxy.conf.js",
    "server:dev": "nodemon server.js",
    "server:prod": "node server.js",
    "start:dev": "npm run build:dev && npm-run-all -p client:dev server:dev",
    "start:prod": "npm run build:prod && npm run server:prod",
    "test:client": "ng test",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "e2e": "npm run config && ng e2e",
    "generate-ssl-certs": "./scripts/generate-ssl-certs.sh",
    "seed": "node ./scripts/seed-db.js"
  },
  "dependencies": {
