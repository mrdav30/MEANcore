    "jimp": "^0.16.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.15",
    "method-override": "^3.0.0",
    "mkdirp": "^1.0.3",
    "moment": "^2.24.0",
    "mongoose": "^5.8.9",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "ng2-file-upload": "^1.4.0",
    "nocache": "^2.1.0",
